@model asp.net.Models.Propietario;
@{
    ViewData["Title"] = "Buscar Propietario";
}

<div class="container my-4">
    <div class="row">
        <div class="col-lg-12">
            <div class="search-container bg-dark text-white p-3 rounded">
                <input type="text" class="form-control" id="q" placeholder="Búsqueda de inquilino..." onkeyup="buscarInquilinoEnVivo(this)" autocomplete="off" />
            </div>
            <div id="resultado-busqueda-inquilinos" class="mt-3">
                <!-- Los resultados de la búsqueda se mostrarán aquí -->
            </div>
        </div>
    </div>
</div>

